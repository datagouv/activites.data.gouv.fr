{
    "version": 2,
    "name": "activites-data-gouv-fr",
    "builds": [{ "src": "package.json", "use": "@now/static-build" }],
    "routes": [
      { "src": "^/js/(.*)", "dest": "/js/$1" },
      { "src": "^/css/(.*)", "dest": "/css/$1" },
      { "src": "^/img/(.*)", "dest": "/img/$1" },
      { "src": "^/assets/(.*)", "dest": "/assets/$1" },
      { "src": ".*", "dest": "/index.html" }
    ],
    "github": {
      "silent": true
    }
  }
  